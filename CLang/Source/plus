(Int a) + (Int b) = intermediate Int
                    (add #a #b)

(Float a) + (Float b) = intermediate Float
                        (add #a #b)

(Int a) + (Float b) = a = ~ a the Float
                      intermediate Float
                      (add #a #b)

(Float a) + (Int b) = b = ~ b the Float
                      intermediate Float
                      (add #a #b)
