~ (Char c) = intermediate Int
             #c

~ (Int n)  = intermediate Char
             #n

~ ({A} seq) = rec (acc = void, i = seq.end)
  if i < 0
    acc the List A
    rec (seq[i] : acc, i - 1)

~ (List A ls)
  rec (seq = seq ls.length the {A}, ls = ls, i = 0)
    if ls.null
      seq
      else rec (set seq[i] to ls.car, ls.cdr, i + 1)

~ ({Char} seq) = read (sum = 0, i = seq.start)
                   if i == seq.end
                     sum
                     else
                     read (sum * 10 + ~ (seq[i] - '0') the Int, i + 1)
