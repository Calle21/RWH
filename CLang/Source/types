type Int    = signed
type Float  = signed
type Access = unsigned
type Bool   = unsigned 1
type Char   = unsigned 8
type Sign   = signed 1
type Fail   = fail

struct Cons E
    E    car
    Ls E cdr

union Ls E = Cons E, Void

void = ()

type Map KE = map (Map KE left
                   K      key
                   E      elt
                   Map KE right) | Void
